{
    "name": "Dataiku DSS",
    "id": "ynhdataikudss",
    "packaging_format": 1,
    "description": {
        "en": "Dataiku DSS is an end-to-end data analysis platform enabling live collaboration on data projects.",
        "fr": "Dataiku DSS est une plateforme d'analyse de données collaborative en temps réel."
    },
    "version": "7.0~ynh1",
    "url": "https://dataiku.com",
    "license": "non-free",
    "maintainer": {
        "name": "Antoine Queru",
        "email": "antoine.queru@dataiku.com",
        "url": "http://dataiku.com"
    },
    "requirements": {
        "yunohost": ">= 3.5"
    },
    "multi_instance": false,
    "services": [
        "nginx",
        "mysql"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain name for Dataiku DSS. Dataiku DSS cannot be installed in a subpath.",
                    "fr": "Choisissez un nom de domaine pour Dataiku DSS. Dataiku DSS ne peut pas être installé dans un sous-chemin."
                },
                "example": "dataiku.example.com"
            },
            {
                "name": "admin",
                "type": "user",
                "ask": {
                    "en": "Choose an admin user",
                    "fr": "Choisissez l’administrateur"
                },
                "example": "johndoe"
            }
        ]
    }
}