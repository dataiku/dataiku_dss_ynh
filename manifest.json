{
    "name": "Dataiku DSS",
    "id": "ynhdataikudss",
    "packaging_format": 1,
    "description": {
        "en": "Dataiku DSS is an end-to-end data analysis platform enabling live collaboration on data projects.",
        "fr": "Dataiku DSS est une plateforme d'analyse de données collaborative en temps réel."
    },
    "version": "7.0~ynh1",
    "url": "https://dataiku.com",
    "license": "non-free",
    "maintainer": {
        "name": "Antoine Queru",
        "email": "antoine.queru@dataiku.com",
        "url": "http://dataiku.com"
    },
    "requirements": {
        "yunohost": ">= 3.5"
    },
    "multi_instance": false,
    "services": [
        "nginx",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain name for Dataiku DSS",
                    "fr": "Choisissez un nom de domaine pour Dataiku DSS"
                },
                "example": "dataiku.example.com"
            },
            {
                "name": "path",
                "type": "path",
                "ask": {
                    "en": "Choose a path for Dataiku DSS",
                    "fr": "Choisissez un chemin pour Dataiku DSS"
                },
                "example": "/dataiku_dss",
                "default": "/dataiku_dss"
            },
            {
                "name": "admin",
                "type": "user",
                "ask": {
                    "en": "Choose an admin user",
                    "fr": "Choisissez l’administrateur"
                },
                "example": "johndoe"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "ask": {
                    "en": "Is it a public application?",
                    "fr": "Est-ce une application publique ?"
                },
                "default": true
            }
        ]
    }
}
