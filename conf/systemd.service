[Unit]
Description=Dataiku DSS service
After=network.target

[Service]
Type=simple
User=YNH_DATAIKU_DSS_USER
Group=YNH_DATAIKU_DSS_USER
WorkingDirectory=YNH_DATAIKU_DSS_PATH/
ExecStart=YNH_DATAIKU_DSS_FINAL_PATH/script >> /var/log/YNH_DATAIKU_DSS_PATH/YNH_DATAIKU_DSS_PATH.log 2>&1

[Install]
WantedBy=multi-user.target
