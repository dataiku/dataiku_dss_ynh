[Unit]
Description=Dataiku DSS service
After=network.target

[Service]
Type=simple
User=YNH_DATAIKU_DSS_USER
Group=YNH_DATAIKU_DSS_USER
WorkingDirectory=YNH_DATAIKU_DSS_FINAL_PATH/
ExecStart=YNH_DATAIKU_DSS_FINAL_PATH/dss_home/bin/dss start >> /var/log/YNH_DATAIKU_DSS_USER/YNH_DATAIKU_DSS_USER.log

[Install]
WantedBy=multi-user.target
